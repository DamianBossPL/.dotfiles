profile Normal {
    output eDP-1 enable mode 1920x1080@60 scale 1
}
profile HDMI {
    output eDP-1 disable
    output HDMI-A-1 enable mode 2560x1440 scale 1
}
profile HDMI-1080 {
    output eDP-1 disable
    output HDMI-A-1 enable mode 1920x1080 scale 1
}
profile DP {
    output eDP-1 disable
    output DP-1 enable mode 2560x1440 scale 1
}
profile DP-1080 {
    output eDP-1 disable
    output DP-1 enable mode --custom 1920x1080 scale 1
}
